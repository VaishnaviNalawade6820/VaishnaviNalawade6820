//Convert the Temperature from Celsius to Fahrenheit.
#include<stdio.h>
int main(){
    float c;
    float far;
    printf("enter temperture in degree celcius(c): ");
    scanf("%f", &c);
    far=1.8*c+32;
    printf("the requried tempreture in faranite=%f", far);
    //printf("\nthe requrired strain=%f", strain);
}
